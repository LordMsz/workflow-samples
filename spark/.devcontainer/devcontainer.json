{
    "dockerComposeFile": "docker-compose.yml",
    "service": "dev",
    "workspaceFolder": "/workspace",
    "postCreateCommand": "bash -i -c 'bash ./.devcontainer/post_creation_script.sh'",
    "customizations": {
        "vscode": {
            "settings": {
                "python.pythonPath": "/usr/local/bin/python3"
            }
        },
        "extensions": [
            "ms-python.python",
            "ms-python.vscode-pylance"
        ]
    }
}